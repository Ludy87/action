{
  "name": "pr-issues-title-verify",
  "version": "1.0.0",
  "private": true,
  "description": "PR & Issues title check GitHub action",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "ncc build ./src/index.ts --out dist --source-map",
    "package": "ncc build ./src/index.ts --out dist --license licenses.txt",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "lint:tsc": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "tsc --noEmit",
    "all": "npm run build && npm run lint:fix && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ludy87/action/pr-issues-title-check"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "PR",
    "Issues",
    "title",
    "verify",
    "check"
  ],
  "author": "Ludy87",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.1",
    "@actions/github": "^6.0.1",
    "@vercel/ncc": "^0.38.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.10.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "js-yaml": "^4.1.1",
    "npm-check-updates": "^19.2.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
